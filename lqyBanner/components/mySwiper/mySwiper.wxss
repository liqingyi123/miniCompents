  display: flex;flex-direction: column;justify-content: center;align-items: flex-start;overflow:unset;
}
.swiper-item image,.swiper-item view{
  transition:all .5s;margin: 0rpx 30rpx;z-index: 1;
}
.active{transform: scale(1.14);z-index: 20; }